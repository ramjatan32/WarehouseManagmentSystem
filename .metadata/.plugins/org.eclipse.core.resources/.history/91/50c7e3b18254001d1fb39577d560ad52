package com.app.warehouse;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

@SpringBootApplication
public class WarehouseAppApplication {

	public static void main(String[] args) {
		SpringApplication.run(WarehouseAppApplication.class, args);

		//approach-1
		 String str="RamjatanKumar";
		 	char []charr=str.toCharArray();
		 	for(int i=charr.length-1;i>=0;i--)
		 	{
		 		System.out.print(charr[i]);
		 	}
		 	System.out.println();
		 	//approach-2
		 	for(int j=str.length()-1;j>=0;j--)
		 	{
		 		System.out.print(str.charAt(j));
		 	}
		 	System.out.println();
		 	// approach-3
		 StringBuffer sb=new StringBuffer(str);
		 System.out.print(sb.reverse());
		 	
		 StringBuilder sbi=new StringBuilder(str);
		 System.out.println(sbi.reverse());
		 
	}
}
